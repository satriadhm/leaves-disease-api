{
  "name": "plant-disease-prediction-api",
  "version": "2.0.0",
  "description": "API untuk prediksi penyakit tanaman menggunakan AI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "seed": "node app/seeds/seed.js",
    "seed:dev": "NODE_ENV=development node app/seeds/seed.js",
    "seed:prod": "NODE_ENV=production node app/seeds/seed.js",
    "seed:force": "node app/seeds/seed-force.js",
    "test:swagger": "node -e \"const axios = require('axios'); (async () => { try { const res = await axios.get('http://localhost:8000/api/docs/swagger.json'); console.log('✅ Swagger JSON OK:', res.data.info.title); } catch(e) { console.log('❌ Swagger failed:', e.message); } })()\"",
    "test:health": "curl -s http://localhost:8000/health | jq .",
    "test:vercel": "curl -s https://leaves-disease-api.vercel.app/health | jq .",
    "test:vercel-swagger": "curl -s https://leaves-disease-api.vercel.app/api/docs/swagger.json | jq .info",
    "debug": "DEBUG=express:* node server.js",
    "build": "echo 'No build step required'",
    "vercel-build": "echo 'Vercel build completed'"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-backend-cpu": "^4.15.0",
    "@vercel/blob": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "axios": "^1.5.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "plant-disease",
    "tensorflow",
    "api",
    "ai",
    "prediction"
  ],
  "author": "G. Satria",
  "license": "ISC"
}
